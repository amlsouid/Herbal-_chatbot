@app.route('/chat', methods=['POST'])
def chat():
    user_message = request.json.get('message', '').strip().lower()
    
    if not user_message:
        return jsonify({'response': 'Please type a message'})
    
    # Process the message
    response = process_message(user_message)
    
    return jsonify({'response': response})

def process_message(message):
    """Process user message and generate response"""
    
    # Greetings
    greetings = ['hello', 'hi', 'hey', 'hola', 'greetings']
    if any(greet in message for greet in greetings):
        return "Hello! üëã I'm your Herbal Medicine Assistant. I can help you learn about medicinal herbs and their benefits. What would you like to know?"
    
    # List all herbs
    if 'list' in message or 'show all' in message or 'all herbs' in message:
        herbs = get_all_herbs()
        return f"Available herbs in my knowledge base: {', '.join(herbs)}"
    
    # Search for specific herb
    for herb in get_all_herbs():
        if herb in message:
            herb_info = search_herb(herb)
            if herb_info:
                response = f"üåø Information about {herb.title()}:\n"
                response += f"Scientific Name: {herb_info['scientific_name']}\n\n"
                response += "Benefits:\n"
                for benefit in herb_info['benefits']:
                    response += f"‚Ä¢ {benefit}\n"
                response += "\nCommon Uses:\n"
                for use in herb_info['uses']:
                    response += f"‚Ä¢ {use}\n"
                response += f"\n‚ö†Ô∏è Precautions: {herb_info.get('precautions', 'No specific precautions')}"
                return response
    
    # Search by benefit
    benefit_pattern = r'for (.*?)(?:\?|\.|$)'
    benefit_match = re.search(benefit_pattern, message)
    if benefit_match:
        benefit = benefit_match.group(1)
        matching_herbs = search_by_benefit(benefit)
        if matching_herbs:
            response = f"Herbs that help with {benefit}:\n"
            for herb in matching_herbs:
                response += f"‚Ä¢ {herb['name'].title()}: {herb['benefit']}\n"
            return response
    
    # Search by symptom
    symptom_keywords = ['headache', 'digestion', 'sleep', 'immunity', 'inflammation', 'nausea', 'stress', 'cold']
    for symptom in symptom_keywords:
        if symptom in message:
            matching_herbs = search_by_symptom(symptom)
            if matching_herbs:
                response = f"Herbs that may help with {symptom}:\n"
                for herb in matching_herbs:
                    herb_info = search_herb(herb)
                    response += f"‚Ä¢ {herb.title()}: {herb_info['benefits'][0]}\n"
                return response
    
    # Help command
    if 'help' in message or 'what can you do' in message:
        return """I can help you with:
‚Ä¢ Showing list of available herbs
‚Ä¢ Searching for specific herbs (e.g., ginger, mint)
‚Ä¢ Finding herbs by benefits (e.g., herbs for digestion)
‚Ä¢ Recommending herbs for symptoms
‚Ä¢ Providing usage information and precautions

Try asking: 'What are the benefits of ginger?' or 'Show me all herbs'"""
    
    # Default response
    return "I'm not sure I understand. You can ask me about specific herbs, their benefits, or ask for 'help' to see what I can do."

